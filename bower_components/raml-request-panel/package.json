{
  "name": "raml-request-panel",
  "version": "0.3.2",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "The request panel view for the request defined as a RAML method",
  "devDependencies": {
    "polymer-cli": "^1.2.0",
    "bower": "^1.8.0"
  },
  "private": false,
  "main": [
    "raml-request-panel.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint raml-request-panel.html raml-request-panel-simple-xhr.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"raml-request-panel:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && node_modules/.bin/bower install --quiet",
    "update-deps": "npm update --quiet && node_modules/.bin/bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/raml-request-panel.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/raml-request-panel/issues",
    "email": "arc@mulesoft.com"
  }
}