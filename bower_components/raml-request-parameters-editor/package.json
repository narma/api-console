{
  "name": "raml-request-parameters-editor",
  "version": "0.3.2",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "An element responsible for displaying a form of query / uri parameters",
  "devDependencies": {
    "polymer-cli": "^1.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "raml-request-parameters-editor.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint raml-request-parameters-editor.html raml-request-parameters-form.html query-parameter-input.html raml-request-parameters-model.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"raml-request-parameters-editor:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/raml-request-parameters-editor.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/raml-request-parameters-editor/issues",
    "email": "arc@mulesoft.com"
  }
}